public class integer3 {
    public static void main(String[] args){
        System.out.println("1-1000以内的完全数有：");
        int n,i,m;

        for(n=1;n<=1000;n++){
            m=0;
            for(i=1;i<=n/2;i++){
                if(n%i==0){
                    m=m+i;
                }
            }
            if(m==n){
                System.out.println(m);
            }
        }
    }
}
