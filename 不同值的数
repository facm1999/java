public class integer1 {
    public static void main(String[] args){
        int count=0;
        for(int i=100;i<999;i++){
            int a=i/100;
            int b=i/10%10;
            int c=i%10;
        if(a!=b&&b!=c&&c!=a){
            System.out.println(i);
            count++;
        }
        }
        System.out.println("总共有"+count+"个数");
    }
}
